  "paths": {
    "/users": {
      "post": {
        "tags": [
          "users"
        ],
        "summary": "create user",
        "description": "create a new user",
        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "in": "body",
            "name": "changeUsernameObject",
            "description": "new username",
            "required": false,
            "schema": {
              "type": "object",
              "properties": {
                "name": {
                  "type": "string",
                  "example": "Seu Nome"
                },
                "email": {
                  "type": "string",
                  "example": "SeuEmail@mail.com"
                },
                "password": {
                  "type": "string",
                  "example": "Minhasenha123"
                }
              }
            }
          }
        ],
        "responses": {
          "200": {
            "description": "user created sucessful",
            "schema": {
              "type": "object",
              "properties": {
                "sucess": {
                  "type": "string",
                  "example": "Usu√°rio criado com sucesso!"
                }
              }
            }
          },
          "400": {
            "description": "celebrate input error"
          },
          "406": {
            "description": "database error",
            "schema": {
              "type": "object",
              "properties": {
                "name": {
                  "type": "string",
                  "example": "Database Error"
                },
                "message": {
                  "type": "string",
                  "example": "Campos invalidos"
                }
              }
            }
          }
        }
      }
    }
  },
  "definitions": {
    "InventoryItem": {
      "type": "object",
      "required": [
        "id",
        "name",
        "releaseDate"
      ],
      "properties": {
        "id": {
          "type": "string",
          "format": "uuid",
          "example": "d290f1ee-6c54-4b01-90e6-d701748f0851"
        },
        "name": {
          "type": "string",
          "example": "Widget Adapter"
        },
        "releaseDate": {
          "type": "string",
          "format": "date-time",
          "example": "2016-08-29T09:12:33.001Z"
        }
      }
    }
  }
